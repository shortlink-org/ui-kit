{
  "name": "@shortlink-org/ui-kit",
  "version": "1.8.1",
  "_id": "@shortlink-org/ui-kit@1.8.1",
  "description": "UI kit for ShortLink",
  "readme": "./README.md",
  "type": "module",
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "pnpm storybook dev -p 6006",
    "dev:with-tests": "pnpm test:generate-output && pnpm storybook dev -p 6006",
    "build-storybook": "NODE_ENV=production pnpm storybook build --config-dir .storybook",
    "semantic-release": "pnpm dlx semantic-release",
    "test": "NODE_ENV=test jest",
    "test:vitest": "vitest",
    "test:storybook": "vitest --project=storybook",
    "test:generate-output": "jest --json --outputFile=test-results.json",
    "test:watch": "vitest --watch",
    "chromatic": "pnpm storybook dev -p 6006",
    "inspector": "pnpm dlx @eslint/config-inspector",
    "lint": "pnpm dlx eslint ./ --max-warnings 0 --report-unused-disable-directives --cache",
    "lint:fix": "pnpm run lint --fix",
    "lint:gitlab": "pnpm dlx lint --format gitlab",
    "prettier": "pnpm dlx prettier -w -c .",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "@shortlink-org:shortlink": "https://gitlab.com/api/v4/projects/15775100/packages/npm/"
  },
  "repository": {
    "url": "https://github.com/shortlink-org/shortlink"
  },
  "keywords": [
    "darkmode",
    "ui-kit"
  ],
  "author": {
    "name": "@batazor",
    "url": "Login Viktor"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@fontsource/caveat": "^5.2.8",
    "@fontsource/material-icons": "^5.2.7",
    "@fontsource/roboto": "^5.2.9",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/types": "^7.4.10",
    "@mui/x-date-pickers": "8.26.0",
    "@tailwindcss/postcss": "4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-table": "8.20.6",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/react": "^19.2.10",
    "autoprefixer": "^10.4.24",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "export-to-csv": "^1.4.0",
    "glob": "^13.0.0",
    "gsap": "3.13.0",
    "motion": "^12.29.2",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-wrap-balancer": "^1.1.1",
    "styled-jsx": "^5.1.7",
    "tailwindcss": "4.1.18",
    "vaul": "^1.1.2",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-a11y": "^10.2.3",
    "@storybook/addon-links": "^10.2.3",
    "@storybook/addon-themes": "^10.2.3",
    "@storybook/addon-vitest": "^10.2.3",
    "@storybook/builder-vite": "^10.2.3",
    "@storybook/nextjs": "^10.2.3",
    "@storybook/nextjs-vite": "^10.2.3",
    "@storybook/react-vite": "^10.2.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "chromatic": "^13.3.5",
    "css-loader": "^7.1.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-storybook": "10.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-sonar-reporter": "^2.0.0",
    "playwright": "^1.58.1",
    "postcss-loader": "^8.2.0",
    "storybook": "^10.2.3",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.2"
}
